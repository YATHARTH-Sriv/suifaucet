
DATABASE_URL="postgresql://username:password@localhost:5432/suifaucet"
DIRECT_URL="postgresql://username:password@localhost:5432/suifaucet"

# Faucet Configuration
# Generate a new Sui keypair for the faucet using Sui CLI:
# 1. sui client new-address ed25519
# 2. sui keytool export <address> key-pair
# Copy the "Private Key" field (base64 encoded string)
# NOTE: The app automatically handles 32/33 byte key differences
FAUCET_PRIVATE_KEY="your_base64_private_key_here"

REDIS_URL="redis://localhost:6379"

NODE_ENV="development"
